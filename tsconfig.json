{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "stripInternal": true,
    "declaration": true,
   // "strictNullChecks": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@angular/core": ["node_modules/@angular/core"],
      "rxjs/*": ["node_modules/rxjs/*"],
      "socket-front-update": [
        "dist/socket-front-update/socket-front-update",
        "dist/socket-front-update"
      ]
    }
  },
  "angularCompilerOptions": {
    "emitDecoratorMetadata": true,
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "enableIvy": false,
    "strictMetadataEmit": true
  }
}
